{
  "name": "quintro",
  "version": "1.0.0",
  "private": true,
  "description": "Classic marble game in HTML",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node ./server/bin/www.js",
    "start:dev": "DEBUG=quintro:* nodemon ./server/bin/www.js",
    "dev-server": "webpack-dev-server --hot --inline --watch --env=dev",
    "build-static": "if-env NODE_ENV=production && npm run build-static:prod || npm run build-static:dev",
    "build-static:dev": "webpack --env=dev",
    "build-static:prod": "webpack -p --env=prod",
    "build-sockets-app": "cd ./server/apps/socket && npm install",
    "postinstall": "sh ./build-scripts/postinstall.sh",
    "lint": "eslint --ext .jsx --ext .js .; exit 0",
    "test": "mocha ./test; exit 0"
  },
  "author": "Turner Hayes <dev@turnerhayes.com>",
  "license": "ISC",
  "engines": {
    "node": "6.9.4"
  },
