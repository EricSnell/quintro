{
  "name": "quintro",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "0.12.4"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start-websocket": "node ./websocket-server.js",
    "start-inspector": "DEBUG=quintro:* DEBUG_DB=1 node-debug ./bin/www",
    "compile-static": "gulp static",
    "postinstall": "cd public && npm install && cd .. && gulp static",
    "nodemon": "nodemon ./bin/www",
    "debug": "DEBUG=quintro:* npm run nodemon",
    "startall": "DEBUG=quintro:* DEBUG_DB=1 concurrent --prefix=command -l 20 \"nodemon bin/www\" \"nodemon websocket-server.js\"",
    "test": "cd test && npm run all"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "connect-mongo": "^0.8.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-hbs": "^0.8.4",
    "express-session": "^1.11.3",
    "gulp": "^3.9.0",
    "handlebars": "^4.0.4",
    "less-middleware": "1.0.x",
    "lodash": "^3.10.1",
    "log4js": "^0.6.26",
    "mongoose": "^4.1.5",
    "morgan": "~1.6.1",
    "passport": "^0.3.0",
    "passport-facebook": "^2.0.0",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.3.6"
  },
  "devDependencies": {
    "babelify": "^6.2.0",
    "browserify": "^11.0.1",
    "concurrently": "^0.1.1",
    "gulp-babel": "^5.2.1",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.0",
    "gulp-jshint": "^1.11.2",
    "gulp-less": "^3.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.6",
    "handlebars": "^3.0.3",
    "hbsfy": "^2.2.1",
    "jshint-stylish": "^2.0.1",
    "less-plugin-autoprefix": "^1.4.2",
    "less-plugin-clean-css": "^1.5.1",
    "merge-stream": "^1.0.0",
    "recursive-readdir": "^1.3.0",
    "run-sequence": "^1.1.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1"
  },
  "description": "Classic marble game in HTML",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turnerhayes/quintro.git"
  },
  "author": "Turner Hayes <dev@turnerhayes.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/turnerhayes/quintro/issues"
  },
  "homepage": "https://github.com/turnerhayes/quintro#readme"
}
